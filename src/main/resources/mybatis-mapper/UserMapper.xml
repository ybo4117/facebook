<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.koreait.facebook.user.UserMapper">

    <insert id="join">
        INSERT INTO t_user
        (email, pw, nm, tel)
        VALUES
        (#{email}, #{pw}, #{nm}, #{tel})
    </insert>

</mapper>
